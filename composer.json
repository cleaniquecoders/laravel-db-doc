{
    "name": "bekwoh/laravel-db-doc",
    "description": "Generate Database Schema Documentation",
    "keywords": [
        "bekwoh",
        "laravel",
        "laravel-db-doc"
    ],
    "homepage": "https://github.com/bekwoh/laravel-db-doc",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim Bin Mohamad",
            "email": "nasrulhazim.m@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.4",
        "illuminate/contracts": "^9.0 | ^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5 | ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bekwoh\\LaravelDbDoc\\": "src",
            "Bekwoh\\LaravelDbDoc\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bekwoh\\LaravelDbDoc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bekwoh\\LaravelDbDoc\\LaravelDbDocServiceProvider"
            ],
            "aliases": {
                "LaravelDbDoc": "Bekwoh\\LaravelDbDoc\\Facades\\LaravelDbDoc"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
